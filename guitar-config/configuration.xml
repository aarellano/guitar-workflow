		<FullComponent>
			<Component>
				<Attributes>
					<Property>
						<Name>Title</Name>
						<Value>Cancel</Value>
					</Property>
				</Attributes>
			</Component>
		</FullComponent>

		<FullComponent>
			<Component>
				<Attributes>
					<Property>
						<Name>Title</Name>
						<Value>Yes</Value>
					</Property>
				</Attributes>
			</Component>
